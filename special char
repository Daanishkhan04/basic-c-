// Online C compiler to run C program online
#include <stdio.h>

void swap(int *a,int *b) 
{
int temp;
temp=*a;
*a=*b;
*b=temp;
}
 main()
{
int a,b;
printf("enter the value of a,b");
scanf("%d%d",&a,&b);
printf("\n-----before swpping------\n");
printf(".....a=%d,b=%d.....",a,b);
swap(&a,&b);
printf("\n----after swapping----\n");
printf(".....a=%d,b=%d.....",a,b);
}
